<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:motan="http://api.weibo.com/schema/motan"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
   http://api.weibo.com/schema/motan http://api.weibo.com/schema/motan.xsd">

    <motan:protocol id="hotelProtocol" name="hotelProtocol"  maxWorkerThread="800" minWorkerThread="20" requestTimeout="3000"/>
    <motan:registry regProtocol="zookeeper" id="my_zookeeper" address="${zk_connect}" />
    <motan:basicService id="basicService" module="hotel" group="group1" registry="my_zookeeper" host="10.10.152.97" shareChannel="true" export="9820"></motan:basicService>
    <!-- exporting service by Motan -->
    <motan:service interface="com.mk.hotel.hotelinfo.HotelService" ref="hotelServiceImpl" basicService="basicService" />
    <motan:service interface="com.mk.hotel.roomtype.RoomTypeService" ref="roomTypeServiceImpl" basicService="basicService"/>
    <motan:service interface="com.mk.hotel.roomtype.RoomTypeStockService" ref="roomTypeStockServiceImpl" basicService="basicService"/>
    <motan:service interface="com.mk.hotel.roomtype.RoomTypePriceService" ref="roomTypePriceServiceImpl" basicService="basicService"/>

</beans>