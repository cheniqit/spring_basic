<?xml version="1.0" encoding="GBK"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">


	<bean id="rocketmqConfig" class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
		<property name="properties">
			<props>
				<prop key="namesrvAddr">${rocketmq.namesrvAddr}</prop>
				<prop key="producerGroupName">hotelProducerGroup</prop>
				<prop key="instanceName">hotelProducer</prop>
			</props>
		</property>
	</bean>
</beans>
